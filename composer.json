{
    "name": "app/common",
    "description": "Common Lib",
    "authors": [
        {
            "name": "Teddy Dona",
            "email": "teddy.dona@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^6.3",
        "symfony/filesystem": "^6.3",
        "symfony/finder": "^6.3",
        "pimple/pimple": "^3.5",
        "defuse/php-encryption": "^2.4",
        "symfony/yaml": "^6.3",
        "laminas/laminas-db": "^2.18",
        "laminas/laminas-inputfilter": "^2.27",
        "laminas/laminas-code": "^4.11",
        "mongodb/mongodb": "^1.16"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.3",
        "monolog/monolog": "~3.4",
        "friendsofphp/php-cs-fixer": "v3.23.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "demo/"
        },
        "psr-4": {
            "App\\Model\\": "demo/models",
            "Tests\\App\\": "tests/src"
        }
    },
    "bin": [
        "appcmd"
    ]
}