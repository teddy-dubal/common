{
    "name": "app/common",
    "description": "Common Lib",
    "authors": [
        {
            "name": "Teddy Dona",
            "email": "teddy.dona@gmail.com"
        }
    ],
    "require": {
        "symfony/filesystem": "^v7.2.0",
        "symfony/finder": "^v7.2.2",
        "pimple/pimple": "^3.5",
        "defuse/php-encryption": "^2.4",
        "symfony/yaml": "^v7.2.5",
        "laminas/laminas-db": "^2.20.0",
        "laminas/laminas-inputfilter": "^2.31.0",
        "laminas/laminas-code": "^4.16.0",
        "mongodb/mongodb": "^2.0.0"
    },
    "require-dev": {
        "symfony/console": "^v7.2.5",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.3",
        "monolog/monolog": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "demo/"
