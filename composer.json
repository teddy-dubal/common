{
    "name": "app/common",
    "description": "Common Lib",
    "authors": [
        {
            "name": "Teddy Dona",
            "email": "teddy.dona@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/finder": "^6.0",
        "pimple/pimple": "~3.5",
        "defuse/php-encryption": "^2.3",
        "symfony/yaml": "^6.0",
        "laminas/laminas-db": "^2.15",
        "laminas/laminas-inputfilter": "^2.13",
        "laminas/laminas-code": "^4.5",
        "mongodb/mongodb": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "monolog/monolog": "~2.5",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "demo/"
        },
        "psr-4": {
            "App\\Model\\": "demo/models",
            "Tests\\App\\": "tests/src"
        }
    },
    "bin": [
        "appcmd"
    ]
}