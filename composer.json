{
    "name": "app/common",
    "description": "Common Lib",
    "authors": [
        {
            "name": "Teddy Dona",
            "email": "teddy.dona@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^5.2",
        "symfony/filesystem": "^5.2",
        "symfony/finder": "^5.2",
        "pimple/pimple": "~3.0",
        "defuse/php-encryption": "^2.3",
        "symfony/yaml": "^5.2",
        "laminas/laminas-db": "^2.12",
        "laminas/laminas-inputfilter": "^2.12",
        "laminas/laminas-code": "^4.3",
        "mongodb/mongodb": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "v1.6.0",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1",
        "monolog/monolog": "~1.7",
        "friendsofphp/php-cs-fixer": "v2.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "demo/"
        },
        "psr-4": {
            "App\\Model\\": "demo/models",
            "Tests\\App\\": "tests/src"
        }
    },
    "bin": [
        "appcmd"
    ]
}
